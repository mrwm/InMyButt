{
  "name": "InMyButt",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Adds the text 'in my butt' after page titles",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
